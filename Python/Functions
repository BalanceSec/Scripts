#!/bin/python3

#Functions
print('\n')
print('Functions')

def who_am_i(): #this is a function without parameters
	name = 'NTP Agent'
	age = '69'
	print("My name is " + name + " and I am " + str(age) + " years old.")

who_am_i()


def add_one_hundred(num):
	print(num + 100)

add_one_hundred(3)

def add_numbers(x,y):
	print(x+y)

add_numbers(3, 9)

def multiply(x,y):
	return x * y

def square_root(x):
	print(x ** .5)

square_root(64)

def nl():
	print('\n')
nl()
